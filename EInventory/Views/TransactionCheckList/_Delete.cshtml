@model EInventory.Models.TransactionChecklist


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel">Are you sure to delete this Checklist ?</h4>
    </div>
    <div class="modal-body">

        <div class="form-group">
            @Html.DisplayNameFor(model => model.OrdNo)
            @Html.DisplayFor(model => model.OrdNo)
        </div>

        <div class="form-group">
            @Html.DisplayNameFor(model => model.ChklstDesc)
            @Html.DisplayFor(model => model.ChklstDesc)
        </div>
    </div>

    <div class="modal-footer">
        <button class="btn" type="button" data-dismiss="modal">No</button>
        <input class="btn btn-danger" type="submit" value="Yes" />
    </div>
}
