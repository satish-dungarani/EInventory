@model EInventory.Models.PartyAddress


<div class="modal-body">
    <div class="row">
        <div class="col-xs-12">
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" id="myModalLabel">Are you sure to delete this Address detail?</h4>
                </div>


                <div class="box-body">
                    <div class="form-group">
                        <div class="col-sm-2 control-label">
                            @Html.DisplayNameFor(model => model.ParamVal)
                        </div>
                        <div class=" control-label">
                            @Html.DisplayFor(model => model.ParamVal, "", new { @class = "control-label" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-2 control-label">
                            @Html.DisplayNameFor(model => model.Addr)
                        </div>
                        <div class=" control-label">
                            @Html.DisplayFor(model => model.Addr, "", new { @class = "control-label" })
                        </div>
                    </div>
                </div>


                <div class="modal-footer">
                    <button class="btn" type="button" data-dismiss="modal">No</button>
                    <input class="btn btn-danger" type="submit" value="Yes" />
                </div>
            }
        </div>
    </div>
</div>
