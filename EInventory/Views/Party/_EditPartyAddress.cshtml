                        @Html.ValidationMessageFor(model => model.refLocID_ParamVal, "", new { @class = "text-danger" })
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword1">@Html.LabelFor(model => model.Addr)</label>
                        @Html.TextBoxFor(model => model.Addr, new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.Addr, "", new { @class = "text-danger" })
                    </div>


                    <div class="form-group">
                        <label for="exampleInputPassword1">@Html.LabelFor(model => model.City, new { @class = "col-sm-2 control-label" })</label>
